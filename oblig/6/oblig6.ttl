[ owl:intersectionOf ( :WarmbloodedHorse
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasCompeted ;
                         owl:someValuesFrom :MonteRace
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
