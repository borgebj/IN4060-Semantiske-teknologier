@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix fam: <http://www.ifi.uio.no/IN3060/family#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sim: <http://www.ifi.uio.no/IN3060/simpsons#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sim:TheSimpsons a fam:Family ;
    fam:hasFamilyMember sim:Bart,
        sim:Homer,
        sim:Lisa,
        sim:Maggie,
        sim:Marge .

sim:Herb a foaf:Person ;
    fam:hasFather [ ] .

sim:Mona a foaf:Person ;
    fam:hasSpouse sim:Abraham ;
    foaf:age "70"^^xsd:int ;
    foaf:name "Mona Simpson" .

sim:Patty a foaf:Person ;
    fam:hasSister sim:Selma .

sim:Selma a foaf:Person ;
    fam:hasSister sim:Patty .

sim:Abraham a fam:Old,
        foaf:Person ;
    fam:hasSpouse sim:Mona ;
    foaf:age "78"^^xsd:int ;
    foaf:name "Abraham Simpson" .

sim:Bart a fam:Minor,
        foaf:Person ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge ;
    fam:hasSister sim:Lisa,
        sim:Maggie ;
    foaf:age "10"^^xsd:int ;
    foaf:name "Bart Simpson" .

sim:Lisa a fam:Minor,
        foaf:Person ;
    fam:hasBrother sim:Bart ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge ;
    fam:hasParent [ fam:hasSister sim:Patty,
                sim:Selma ],
        [ fam:hasBrother sim:Herb ] ;
    fam:hasSister sim:Maggie ;
    foaf:age "8"^^xsd:int ;
    foaf:name "Lisa Simpson" .

sim:Maggie a fam:Infant,
        fam:Minor,
        foaf:Person ;
    fam:hasBrother sim:Bart ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge ;
    fam:hasParent [ fam:hasFather sim:Abraham ],
        [ fam:hasMother sim:Mona ] ;
    fam:hasSister sim:Lisa ;
    foaf:age "1"^^xsd:int ;
    foaf:name "Maggie Simpson" .

sim:Homer a foaf:Person ;
    fam:hasFather sim:Abraham ;
    fam:hasSpouse sim:Marge ;
    foaf:age "36"^^xsd:int ;
    foaf:name "Homer Simpson" .

sim:Marge a foaf:Person ;
    fam:hasSpouse sim:Homer ;
    foaf:age "34"^^xsd:int ;
    foaf:name "Marge Simpson" .

